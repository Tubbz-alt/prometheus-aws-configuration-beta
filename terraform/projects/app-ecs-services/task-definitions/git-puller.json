[
  {
    "name": "git-puller",
    "image": "alpine/git",
    "cpu": 40,
    "memory": 256,
    "essential": true,
    "mountPoints": [
      {
        "sourceVolume": "pulled-config",
        "containerPath": "/configs"
      }
    ],
    "command": ["clone", "https://github.com/alphagov/prometheus-config.git", "/configs"],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}"
      }
    }
  }
]
